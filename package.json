    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "cowsay": "1.3.1",
    "cross-env": "5.2.0",
    "date-now-cli": "1.1.0",
    "jest": "23.5.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-dom": "1.12.0",
    "jest-junit": "5.1.0",
